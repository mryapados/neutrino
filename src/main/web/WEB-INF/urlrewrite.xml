<urlrewrite>
    <rule>
       <from>^/admin/$</from>
       <to>/admin/index</to>
    </rule>

    <rule>
       <from>^/index.html$</from>
       <to>/</to>
    </rule>


    <rule>
       <from>^/article-([0-9])/$</from>
       <to>/article/article?id=$1</to>
    </rule>
    <rule>
       <from>^/([a-zA-Z0-9_\-]+)/$</from>
       <to>/project/project?p=$1</to>
    </rule>

    <rule>
       <from>^/([a-zA-Z0-9_\-]+).html$</from>
       <to>/page/page?pg=$1</to>
    </rule>

    <rule>
       <from>^/demo/api.do?method=getUser&amp;uid=(.*)$</from>
       <to>/demo/api/user/$1</to>
    </rule>
</urlrewrite>


