<urlrewrite>

    <rule>
       <from>^/admin/$</from>
       <to>/admin/index</to>
    </rule>

    <rule>
       <from>^/index.html$</from>
       <to>/</to>
    </rule>
    
    <rule>
       <from>^/bo/(.*)$</from>
       <to>/@bo/$1</to>
    </rule>
    

	<!-- With keywords -->
    <rule>
       <from>^/@([a-zA-Z0-9_\-]+)/([a-zA-Z0-9_\-]+).html$</from>
       <to>/page/page?pg=$2</to>
    </rule>
    
    <!-- Without keywords -->
    <rule>
       <from>^/@([a-zA-Z0-9_\-]+).html$</from>
       <to>/page/page?pg=$1</to>
    </rule>


	<!-- With keywords -->
    <rule>
       <from>^/([a-zA-Z0-9_\-]+)/article-([0-9])/$</from>
       <to>/article/article?id=$2</to>
    </rule>
    <rule>
       <from>^/([a-zA-Z0-9_\-]+)/([a-zA-Z0-9_\-]+)/$</from>
       <to>/project/project?p=$2</to>
    </rule>
    <rule>
       <from>^/([a-zA-Z0-9_\-]+)/([a-zA-Z0-9_\-]+).html$</from>
       <to>/category/category?cat=$2</to>
    </rule>

	<!-- Without keywords -->
    <rule>
       <from>^/article-([0-9])/$</from>
       <to>/article/article?id=$1</to>
    </rule>
    <rule>
       <from>^/([a-zA-Z0-9_\-]+)/$</from>
       <to>/project/project?p=$1</to>
    </rule>
    <rule>
       <from>^/([a-zA-Z0-9_\-]+).html$</from>
       <to>/category/category?cat=$1</to>
    </rule>






    <rule>
       <from>^/demo/api.do?method=getUser&amp;uid=(.*)$</from>
       <to>/demo/api/user/$1</to>
    </rule>
</urlrewrite>


